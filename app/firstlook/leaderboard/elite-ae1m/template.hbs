<div class="row" >
  <div class="col-lg-6 leaderboard" style="text-align: left;"><h1>LEADERBOARD<span class="dkBlue">/AE</span></h1><p class="footnote" style="padding-left: 5px;">Ranking is based on % to goal</p></div>
  <div class="col-lg-6 eliteNav" style="">
    {{#link-to 'firstlook.leaderboard.elite-ae15m' replace="true"}}<img src="/assets/img/leaderboard/01_tab.png" />{{/link-to}}
    <img src="/assets/img/leaderboard/02_section.png" />
    {{#link-to 'firstlook.leaderboard.elite-ae700k' replace="true"}}<img src="/assets/img/leaderboard/03_tab.png" />{{/link-to}}
    {{#link-to 'firstlook.leaderboard.elite-ae450k' replace="true"}}<img src="/assets/img/leaderboard/04_tab.png" />{{/link-to}}
    {{#link-to 'firstlook.leaderboard.elite-ae400k' replace="true"}}<img src="/assets/img/leaderboard/05_tab.png" />{{/link-to}}
    <div class="footnote"><p class="" style="">    Select tabs to view Top 8 by Category </p></div>

  </div>
</div>



  <div class="row" class="eliteGroup" >
    {{#each model as |Eliteae1m|}}

          <div class="col-sm-12 col-md-6 col-lg-3 eliteItem"  style="text-align:left; ">
            <div class="image1st" style="">
              <img style="" src="/assets/img/leaderboard/userPhoto.jpg"   >
              <!-- replace image with user photo  -->
              <div class="percent" style="">
                <svg width="80" height="80">
                    <circle cx="40" cy="40" r="20" fill="#0099D8" />
                      <text x="50%" y="50%" text-anchor="middle" fill="white" font-size="13px" font-family="Arial" dy=".35em">{{ Eliteae1m.fullyear }}%</text>
                      <!-- replace text with user percent  -->
                </svg>
            </div>

            </div>

            <div class="eliteProgressBar" style="  ">

                <div class="progress" style="">
                  <div class="progress-bar" role="progressbar" aria-valuenow="{{ Eliteae1m.fullyear }}" aria-valuemin="0" aria-valuemax="100" style=" width:{{ Eliteae1m.fullyear }}%; ">
                  </div>
                </div>

             
           </div>

            <div class="eliteDatum2  " style=" ">
              <span class="ltBlue">Name</span>  <span >{{ Eliteae1m.name}}</span><br><span class="ltBlue">Manager</span> {{ Eliteae1m.managername}}<br> <span class="ltBlue">Division</span> {{ Eliteae1m.division}}<br> <span class="ltBlue">Region</span> {{ Eliteae1m.region}}
            </div>
          </div>

    {{/each}}
    </div>

{{outlet}}

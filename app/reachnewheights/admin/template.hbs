     <div class="row col-lg-7" id="Filters"  >
      <div class=" filters " style="    padding-left: 8px;" >
            <div style="margin:auto; text-align:center;"><p>Select tab to view each divisional roll-up</p></div>
              <div id="divCentral" class="divisionBtn divisionBtn1 divisionBtn1-Active" {{action "filterCategoryCentral"}}>
                &nbsp;</div>
              <div id="divSouth" class="divisionBtn divisionBtn2" {{action "filterCategorySouth"}}>
                  &nbsp;</div>
              <div id="divWest" class="divisionBtn divisionBtn3" {{action "filterCategoryWest"}} >
                  &nbsp;</div>
              <div id="divNortheast" class="divisionBtn divisionBtn4" {{action "filterCategoryNortheast"}}>
                &nbsp;</div>

        </div>
      <!-- <div class="footnote"><p>Select tabs to view by Category</p></div> -->

    </div>
  </div>

  </div>

</section>
<section id="AElist" class="" style="padding-bottom: 0px;" >

  <div id="datatable" class="container-fluid datatable" style=" ">


    <!--   ================================   -->
    <!--  Central table  -->
    <!--   ================================   -->
    <div class="row" id="aeCentral" style="">
      <div class="col-lg-12" style="padding-left:0; padding-right:0;">
        {{models-table
           data=model.central
           columns=columns
           showGlobalFilter=false
           showComponentFooter=false
           filteringIgnoreCase=true
           columnComponents=(hash
            editRow=(component "models-table/cell-edit-toggle" saveRowAction=(action "onSaveRow") cancelRowAction=(action "onCancelRow"))
          )
